    .errors
      - if @messages.present?
        - @messages.each do |msg|
          %p= msg.last
    .col-md-12
      .styled_input.participant_surname{class: (@messages.present? && @messages[:participant_surname]) ? 'error_field' : '' }
        %span.glyphicon.glyphicon-user
        = f.input :surname, placeholder: 'Фамилия', label: false, require: true
      .styled_input.participant_name{class: (@messages.present? && @messages[:participant_name]) ? 'error_field' : '' }
        %span.glyphicon.glyphicon-user
        = f.input :name, placeholder: 'Имя', label: false
      .styled_input.participant_thirdname{class: (@messages.present? && @messages[:participant_thirdname]) ? 'error_field' : '' }
        %span.glyphicon.glyphicon-user
        = f.input :thirdname, placeholder: 'Отчество', label: false
      .styled_input.participant_email{class: (@messages.present? && @messages[:participant_email]) ? 'error_field' : '' }
