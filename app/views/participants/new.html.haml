        = f.input :email, placeholder: 'E-mail', label: false
      .styled_input.participant_phone{class: (@messages.present? && @messages[:participant_phone]) ? 'error_field' : '' }
        %span.glyphicon.glyphicon-earphone
        = f.input :phone, placeholder: 'Телефон', label: false
      .form-group.text-center.participant_birhday{class: (@messages.present? && @messages[:participant_birthday]) ? 'error_field' : '' }
